<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="middleware.app.DecryptionResultsController">
   <children>
      <VBox prefHeight="600.0" prefWidth="800.0">
         <children>
            <AnchorPane prefHeight="141.0" prefWidth="800.0">
